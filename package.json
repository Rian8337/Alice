{
  "name": "alice",
  "version": "2.0.0",
  "description": "A Discord bot for osu!droid",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-windows": "rmdir dist /s /q & npm run compile-build && npm run build-osu-droid-windows",
    "compile-linux": "rm -rf dist & npm run compile-build && npm run build-osu-droid-linux",
    "compile-build": "tsc --build tsconfig.json",
    "build-osu-droid-windows": "dts-gen -m mathfn -o -f ./node_modules/mathfn/mathfn.d.ts && dts-gen -m java-deserialization -o -f ./node_modules/java-deserialization/src/index.d.ts && cd ./modules/osu-droid & rmdir dist /s /q & tsc --build tsconfig.json",
    "build-osu-droid-linux": "dts-gen -m mathfn -o -f ./node_modules/mathfn/mathfn.d.ts && dts-gen -m java-deserialization -o -f ./node_modules/java-deserialization/src/index.d.ts && cd ./modules/osu-droid && tsc --build tsconfig.json",
    "startDebug": "node --trace-warnings --inspect dist/main.js",
    "start": "node --trace-warnings dist/main.js",
    "startHeroku": "pm2-runtime start ecosystem.config.js --env production",
    "heroku-postbuild": "npm run compile-linux"
  },
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "discordjs",
    "typescript",
    "osudroid",
    "osu",
    "osu!",
    "osu!droid",
    "discord.js"
  ],
  "type": "commonjs",
  "author": "Rian8337",
  "license": "MIT",
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/crypto-js": "^4.0.1",
    "@types/d3": "^6.7.0",
    "@types/lodash": "^4.14.175",
    "@types/mongodb": "^3.6.12",
    "@types/request": "^2.48.5",
    "@types/unzipper": "^0.10.3",
    "@types/yt-search": "^2.3.2",
    "dts-gen": "^0.6.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@discordjs/voice": "^0.6.0",
    "adm-zip": "^0.5.5",
    "bufferutil": "^4.0.3",
    "canvas": "^2.8.0",
    "consola": "^2.15.3",
    "crypto-js": "^4.0.0",
    "d3": "^6.7.0",
    "discord.js": "^13.3.0",
    "dotenv": "^9.0.2",
    "html-entities": "^2.3.2",
    "java-deserialization": "^0.1.0",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "mathfn": "^1.2.0",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.6",
    "osu-droid": "file:modules/osu-droid",
    "pm2": "^5.1.1",
    "request": "^2.88.2",
    "unzipper": "^0.10.11",
    "utf-8-validate": "^5.0.5",
    "youtube-dl-exec": "^1.2.6",
    "yt-search": "^2.10.2",
    "zlib-sync": "^0.1.7"
  },
  "_moduleAliases": {
    "@alice-commands": "dist/commands",
    "@alice-core": "dist/core",
    "@alice-database": "dist/database",
    "@alice-enums": "dist/enums",
    "@alice-events": "dist/events",
    "@alice-interfaces": "dist/interfaces",
    "@alice-types": "dist/types",
    "@alice-utils": "dist/utils"
  }
}
