{
  "name": "alice",
  "version": "2.0.0",
  "description": "A Discord bot for osu!droid",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-windows": "npm run build-osu-droid-windows && rmdir dist /s /q & npm run compile-build",
    "compile-linux": "npm run build-osu-droid-linux && rm -rf dist & npm run compile-build",
    "compile-build": "tsc --build tsconfig.json",
    "build-osu-droid-windows": "npm run write-libraries-declaration && cd ./modules/osu-droid & rmdir dist /s /q & tsc --build tsconfig.json",
    "build-osu-droid-linux": "npm run write-libraries-declaration && cd ./modules/osu-droid && tsc --build tsconfig.json",
    "write-libraries-declaration": "dts-gen -m java-deserialization -o -f ./node_modules/java-deserialization/src/index.d.ts",
    "startDebug": "node --trace-warnings --inspect .",
    "start": "node --trace-warnings .",
    "startHeroku": "pm2-runtime start ecosystem.config.js --env production",
    "heroku-postbuild": "npm run compile-linux"
  },
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "discordjs",
    "typescript",
    "osudroid",
    "osu",
    "osu!",
    "osu!droid",
    "discord.js"
  ],
  "type": "commonjs",
  "author": "Rian8337",
  "license": "MIT",
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/crypto-js": "^4.0.1",
    "@types/d3": "^6.7.0",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/request": "^2.48.5",
    "@types/unzipper": "^0.10.3",
    "@types/yt-search": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dts-gen": "^0.6.0",
    "eslint": "^8.3.0",
    "prettier": "^2.5.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@discordjs/voice": "^0.6.0",
    "adm-zip": "^0.5.5",
    "bufferutil": "^4.0.3",
    "canvas": "^2.8.0",
    "consola": "^2.15.3",
    "crypto-js": "^4.0.0",
    "d3": "^6.7.0",
    "discord.js": "^13.3.1",
    "dotenv": "^9.0.2",
    "html-entities": "^2.3.2",
    "java-deserialization": "^0.1.0",
    "libsodium-wrappers": "^0.7.9",
    "lodash.clonedeep": "^4.5.0",
    "module-alias": "^2.2.2",
    "mongodb": "^4.2.0",
    "osu-droid": "file:modules/osu-droid",
    "request": "^2.88.2",
    "unzipper": "^0.10.11",
    "utf-8-validate": "^5.0.5",
    "youtube-dl-exec": "^1.2.6",
    "yt-search": "^2.10.2",
    "zlib-sync": "^0.1.7"
  },
  "_moduleAliases": {
    "@alice-commands": "src/commands",
    "@alice-core": "src/core",
    "@alice-database": "src/database",
    "@alice-enums": "src/enums",
    "@alice-events": "src/events",
    "@alice-interfaces": "src/interfaces",
    "@alice-types": "src/types",
    "@alice-utils": "src/utils"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-inferrable-types": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  }
}
